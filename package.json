{
  "name": "caycore",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "NİBGAT®",
    "email": "contact@nibgat.com",
    "url": "https://nibgat.com/app/karala"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "jquery": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jss": "^10.0.3",
    "react-scripts": "3.0.1"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder --linux deb tar.xz",
    "build": "yarn react-build && yarn electron-build",
    "release": "yarn react-build && electron-builder --publish=always --linux deb tar.xz",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "desktop": "npm run electron-dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^9.1.0",
    "electron-is-dev": "^1.1.0",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "com.nibgat.twitchchat",
    "productName": "Twitch Chat",
    "copyright": "Copyright © 2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
